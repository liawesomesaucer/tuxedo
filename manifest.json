{
  "name": "Tuxedo",
  "description": "Selectively make websites black and white to make them less distracting",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["tuxedo.css"],
      "js": ["tuxedo.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["tabs", "storage"],
  "host_permissions": [
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
